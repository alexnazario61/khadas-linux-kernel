From 1dcb2581f1d41fd49d1af30181b634bf1c05ec08 Mon Sep 17 00:00:00 2001
From: hyphop <email2tema@gmail.com>
Date: Mon, 28 Oct 2019 06:05:39 +0300
Subject: [PATCH] vim3 led gpio fix to hight

---
 arch/arm64/boot/dts/amlogic/meson-khadas-vim3.dtsi | 8 ++++----
 1 file changed, 4 insertions(+), 4 deletions(-)

diff --git a/arch/arm64/boot/dts/amlogic/meson-khadas-vim3.dtsi b/arch/arm64/boot/dts/amlogic/meson-khadas-vim3.dtsi
index 8647da7..ce2e8f1 100644
--- a/arch/arm64/boot/dts/amlogic/meson-khadas-vim3.dtsi
+++ b/arch/arm64/boot/dts/amlogic/meson-khadas-vim3.dtsi
@@ -41,15 +41,15 @@
 	leds {
 		compatible = "gpio-leds";
 
-		white {
+		led_white: white {
 			label = "vim3:white:sys";
-			gpios = <&gpio_ao GPIOAO_4 GPIO_ACTIVE_LOW>;
+			gpios = <&gpio_ao GPIOAO_4 GPIO_ACTIVE_HIGH>;
 			linux,default-trigger = "heartbeat";
 		};
 
-		red {
+		led_red: red {
 			label = "vim3:red";
-			gpios = <&gpio_expander 5 GPIO_ACTIVE_LOW>;
+			gpios = <&gpio_expander 5 GPIO_ACTIVE_HIGH>;
 		};
 	};
 
